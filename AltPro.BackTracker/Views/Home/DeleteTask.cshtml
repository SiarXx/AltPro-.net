@model AltPro.BackTracker.ViewModels.TaskViewModel

@{
    ViewData["Title"] = "DeleteView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="row">
        <div class="col-md-4">
            <div class="alert alert-success" role="alert">
                <h4 class="alert-heading">Well done!</h4>
                <p>You successfully deleted the task.</p>
                <hr />
                <form asp-action="ReportList" asp-controller="Home" method="post">
                    <input hidden asp-for="Id" />
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="ModuleName" class="control-label"></label>
                        <input readonly asp-for="ModuleName" class="form-control" />
                        <span readonly asp-validation-for="ModuleName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="TaskPriority" class="control-label"></label>
                        <select disabled asp-for="TaskPriority" class="custom-select" asp-items="Html.GetEnumSelectList<ETaskPriority>()"></select>
                    </div>
                    <div class="form-group">
                        <label asp-for="Description" class="control-label"></label>
                        <textarea readonly asp-for="Description" class="form-control"></textarea>
                    </div>
                    <div class="form-group">
                        <a class="btn btn-primary" asp-controller="home" asp-action="ReportList"> Back</a>
                    </div>
                </form>
            </div>
        </div>
        <hr>
    </div>

